
public class funcoes_matematicas {

	public static void main(String[] args) {
		 
		double x, y ,z, A, B, C;
		x = 3.0;
		y = 4.0;
		z = -5.0;
		
		A = Math.sqrt(x);
		B = Math.sqrt(y);
		C = Math.sqrt(25.0);
		System.out.println("Raiz quadrada de " + x + " = ", + A);
		System.out.println("Raiz quadrada de " + y + " = ", + B);
		System.out.println("Raiz quadrada de " + (int)z + " = ", + C);
	}

}
